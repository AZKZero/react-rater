{
  "name": "react-rater",
  "version": "0.2.0",
  "description": "A star rater in react.js",
  "main": "src/index.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "build-js": "webpack",
    "build-css": "sass --sourcemap=none src/react-rater.scss dist/react-rater.css",
    "build": "npm run build-js & npm run build-css",
    "build-example-js": "webpack --config example/webpack.config.js",
    "build-example-css": "sass --sourcemap=none example/index.scss example/build.css",
    "build-example": "npm run build-example-js & npm run build-example-css"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NdYAG/react-rater.git"
  },
  "keywords": [
    "react",
    "rater",
    "star-rater",
    "react-component"
  ],
  "author": "NdYAG",
  "license": "BSD",
  "bugs": {
    "url": "https://github.com/NdYAG/react-rater/issues"
  },
  "homepage": "https://github.com/NdYAG/react-rater",
  "dependencies": {
    "react": "^0.14.6",
    "react-dom": "^0.14.6"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-core": "^6.4.5",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.2.0",
    "webpack": "^1.12.12"
  }
}
